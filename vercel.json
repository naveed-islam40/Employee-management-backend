{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "API_URL": "https://api.example.com",
      "NODE_ENV": "production"
    },
    "outputDirectory": "dist"
  }
  